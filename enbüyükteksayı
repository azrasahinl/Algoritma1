
package newpackage;

import java.util.Scanner;

public class ornekalgo1 {
  //10 elemanlı tam sayı dizisindeki en büyük tek tam sayıyı bulan program
    public static void main(String[] args) {
        int dizi[]={1,2,12,4,5,6,7,3,9,10};
        int enbuyukteksayi=-1;
        for (int i =0; i < 10; i++) {
        if(dizi[i]%2!=0 && dizi[i]>enbuyukteksayi){
            enbuyukteksayi=dizi[i];
            
        }
}
        if(enbuyukteksayi==-1)
            System.out.println("Dizide tek sayi yok");
        else
            System.out.println("En buyuk tek sayi:"+enbuyukteksayi);
}
}
