
package algo.örnek.sorular;

import java.util.Scanner;

public class algoörnek272024 {
 //Klavyeden girilen binary bir sayının complementini (tersini) alan java kodunu yazınız?
    public static void main(String[] args) {
        Scanner k=new Scanner(System.in);
        String x=k.next();
        String boş=" ";
        for (int i = 0; i <x.length(); i++) {
           if(x.charAt(i)=='0')
               boş=boş+'1';
           if(x.charAt(i)=='1')
               boş=boş+'0';
        }
        System.out.println(boş);
    }
}
